<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="FixedSizeTextView">
        <attr name="text_size" format="enum">
            <enum name="small" value="0" />
            <enum name="medium" value="1" />
            <enum name="large" value="2" />
        </attr>

        <attr name="text_color" format="enum">
            <enum name="primary" value="0" />
            <enum name="secondary" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="FormattedNumberEditText">
        <attr name="prefix" format="string" />
        <attr name="numberOfGroups" format="integer" />
        <attr name="groupLength" format="integer" />
        <attr name="groupSeparator" format="string" />
    </declare-styleable>
</resources>